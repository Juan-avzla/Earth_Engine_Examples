// Seleccionar bandas para clasificar
var Bands = ["B2", 'B3', 'B4', 'B5', 'B6', 'B7', 'B8', 'B8A', 'B11', 'B12'];
print(Bands);


//Crear un mosaico con las dos imagenes resultantes de la busqueda
var Imagen = imageCollection.mean();

//Mostrar imagenes en el mapa
Map.addLayer(Imagen, {bands: ['B4', 'B3', 'B2'], max: 3000}, 'Imagen Satelital');

Map.addLayer(Imagen, {bands: ['B8', 'B3', 'B2'], max: 3000}, 'Falso color');

Map.addLayer(Imagen, {bands: ['B12', 'B8A', 'B4'], max: 3000}, 'Infrarrojo onda corta');

Map.addLayer(Imagen, {bands: ['B11', 'B8', 'B2'], max: 3000}, 'Agricultura');

Map.addLayer(Imagen, {bands: ['B12', 'B11', 'B2'], max: 3000}, 'Geologia');

Map.addLayer(Imagen, {bands: ['B4', 'B3', 'B1'], max: 3000}, 'Batimetrico');



// Ingresar los puntos de muestreo para formar la clasificacion
// con imagenes sentinel

