// Ingresar el Area de Interes (Ciudad Obregon)
var Ciudad_Obregon = ee.FeatureCollection("users/juanalexis0407/Ciudad_Obregon");

// Seleccion de imagenes satelitales Sentinel-2A

// ID de imagenes a utilizar
// S2A_MSIL2A_20201003T175201_N0214_R141_T12RWR_20201003T221250
// S2A_MSIL2A_20201003T175201_N0214_R141_T12RXR_20201003T221250

// Seleccion de bandas Sentinel2
var BANDS = ['B2', 'B3', 'B4', 'B5', 'B6', 'B7', 'B8', 'B8A', 'B9', 'B11', 'B12'];

// Funcion para crear una mascara de nubes
function maskS2sr(image) {
  var qa = image.select('QA60');
// Excluir los pixeles identificados como "nubes" y "cirrus" de la imagen
  var clipcloud = 1 << 10;
  var clipcirrus = 1 << 11;
  var Mask = qa.bitwiseAnd(clipcloud).eq(0)
        .and(qa.bitwiseAnd(clipcirrus).eq(0));
  return image.updateMask(Mask).select(BANDS).divide(10000);
}

// Realizar la llamada a la coleccion de imagenes Sentinel
var S2A_SR = ee.ImageCollection('COPERNICUS/S2_SR')
// Filtrar los resultados por fecha y utilizando como delimitacin,
// el rea de interes (Ciudad Obregon)
   .filterDate('2020-10-03','2020-10-04')
   .filterBounds (Ciudad_Obregon)
   .map(maskS2sr);
print(S2A_SR);

// Crear las propiedades de visualizacion
var rgbVis = {
  min: 0.0,
  max: 0.3,
  bands: ['B4', 'B3', 'B2'],};
// Mostrar el mosaico de imagenes en Earth Engine
Map.addLayer(S2A_SR.median(), rgbVis, 'SentinelCloudless');


// Ingresar los puntos de muestreo para formar la clasificacion
// con imagenes sentinel

Map.addLayer(RSU_Sitios_Muestreo.draw({color: "red"}),
            {},"Sitios Muestreo");

Map.addLayer(RSU_2020CdObregon.draw({color: "blue"}),
            {}, "RSU 2020");


// Unir las geometrias dentro de una sola variable

var Clases_de_clasificacion = Basureros.merge(Cuerpo_de_agua).merge(Mancha_urbana)
    .merge(Suelo_desnudo).merge(Tierra_de_cultivo).merge(Vegetacion_densa)
    .merge(Area_cultivo_2).merge(Matorral).merge(Calle);
    
  print(Clases_de_clasificacion);

// Unir solo cuatro clases para crear el asset de calsificacion
// (Claes basurero, vegetacion, Cuerpo de agua)
// var Tres_clases = Basureros.merge(Cuerpo_de_agua).merge(Vegetacion_densa);


// Exportar clases en fomato de asset
Export.table.toAsset({
  collection: Clases_de_clasificacion,
  description:'exportacion_de_nueve_clases_para_clasificacion',
  assetId: 'clases_para_clasificacion_supervisada_GEETF',
});

