/***** End of imports. If edited, may not auto-convert in the playground. *****/

// Ingresar el Area de Interes (AOI)
Cajeme = ee.FeatureCollection("users/juanalexis0407/Cajeme");
Ciudad_Obregon = ee.FeatureCollection("users/juanalexis0407/Ciudad_Obregon");


// Seleccion de imagenes satelitales Sentinel-2A

// ID de imagenes a utilizar
// S2A_MSIL2A_20201003T175201_N0214_R141_T12RWR_20201003T221250
// S2A_MSIL2A_20201003T175201_N0214_R141_T12RXR_20201003T221250

// LLamar a la coleccion de imagenes
var imageCollection = ee.ImageCollection("COPERNICUS/S2_SR")
  .filterDate('2020-10-03','2020-10-04')
  .filterBounds(Ciudad_Obregon);

// Imprimir resultado
print(imageCollection);

// Seleccionar las imagenes segun su ID
//var imagen_1 = ee.Image(imageCollection.filterDate('2020-10-03','2020-10-04').filterMetadata('MGRS_TILE','equals','12RWR'));
//var imagen_2 = ee.Image(imageCollection.filterDate('2020-10-03','2020-10-04').filterMetadata('MGRS_TILE','equals','12RXR'));

// Imprimir resultados
// print (imagen_1);
// print(imagen_2);

// Seleccionar los resultados
// var Img_Sentinel_1 = ee.Image("COPERNICUS/S2_SR/20201003T175201_20201003T180540_T12RWR");
// var Img_Sentinel_2= ee.Image("COPERNICUS/S2_SR/20201003T175201_20201003T180540_T12RXR");

// Seleccionar bandas para clasificar
var Bands = ["B2", 'B3', 'B4', 'B5', 'B6', 'B7', 'B8', 'B8A', 'B11', 'B12'];
print(Bands);


//Crear un mosaico con las dos imagenes resultantes de la busqueda
var Imagen = imageCollection.mean();

//Mostrar imagenes en el mapa
Map.addLayer(Imagen, {bands: ['B4', 'B3', 'B2'], max: 3000}, 'Imagen Satelital');

// Ingresar los puntos de muestreo para formar la clasificacion
// con imagenes sentinel

Map.addLayer(RSU_Sitios_Muestreo.draw({color: "red"}),
            {},"Sitios Muestreo");

Map.addLayer(RSU_2020CdObregon.draw({color: "blue"}),
            {}, "RSU 2020");


// Crear las clases a partir de los sitios de muestreo
// Algunos de los sitios del archivo RSU2020 son basureros
// Intraurbanos, cuentan con una resolucion de pixel demasiado
// grande para su clasificacion

// Tomar como referencia el archivo RSU_sitios_Muestreo

// A continuacion se tomaran 20 puntos a los alrededores de cada
// punto de los sitios de muestreo (N = 321 distribuidos en los
// 16 puntos de muestreo)

var Puntos_entrenamiento = Sitios_RSU.merge(Cultivo)

// Creacion de areas o zonas de entrenamiento
var entrenamiento = Imagen.select(Bands).sampleRegions({
  collection: Puntos_entrenamiento,
  properties: ['Cobertura'],
  scale: 10
});
// Imprimir resultados
print(entrenamiento);

// crear clasificador de puntos 
var entrenador_cart = ee.Classifier.cart();
var clasificador = entrenador_cart.train({
  features: entrenamiento,
  classProperty: 'Cobertura',
  inputProperties: Bands
});

var classify = Imagen.classify(clasificador);
print(classify);

// aadir imagen clasificada al mapa
Map.addLayer(classify, {
  palette: ['d63000', '98ff00', '0b4a8b', 'ffc82d'],
  min: 1,
  max: 2},
  'Clasificacion Supervisada'
);
  

// NOTAS
// No es posible realizr una clasificacion supervisada con una
// sola clase. Lo minimo es realizar una clasificacion binaria

// https://stackoverflow.com/questions/53464543/supervised-classification-with-only-one-class-with-google-earth-engine






